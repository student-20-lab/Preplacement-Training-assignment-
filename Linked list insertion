class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next

def insert_at_end(head, x):
    new_node = ListNode(x)
    
    
    if head is None:
        return new_node
    
    
    current = head
    while current.next:
        current = current.next
    

    current.next = new_node
    
    return head
