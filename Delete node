class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next

def delete_alternate_nodes(head):
    if head is None:
        return None
    
    current = head
    while current and current.next:
        
        current.next = current.next.next
        
        current = current.next
    
    return head
